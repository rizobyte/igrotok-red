{
    "name": "Игроток Red",
    "version": "1.0",
    "description": "+1 причина для посещения Игротока",
    "content_scripts": [
        {
            "matches": [
                "https://igrotok.yandex.ru/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "host_permissions": [
        "https://igrotok.yandex.ru/*"
    ],
    "icons": {
        "16": "icons/icon_16x16.png",
        "48": "icons/icon_48x48.png",
        "128": "icons/icon_128x128.png"
    },
    "manifest_version": 2
}